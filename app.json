{
  "expo": {
    "name": "HSS Spares",
    "slug": "hss-spares",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hss-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "hss",
    "splash": {
      "image": "./assets/hss-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e3a8a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hssspares.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/hss-logo.png",
        "backgroundColor": "#1e3a8a"
      },
      "package": "com.hssspares.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f1a4aec7-e3ba-437c-a7ca-f19e7ce72751"
      }
    },
    "owner": "kevmoore1975"
  }
}
