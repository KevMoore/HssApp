{
  "expo": {
    "name": "HSS Spares",
    "slug": "hss-spares",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hss-logo-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "hss",
    "splash": {
      "image": "./assets/hss-logo-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e3a8a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hssspares.app",
      "appleTeamId": "T6R6644E7Y",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/hss-logo-icon.png",
        "backgroundColor": "#1e3a8a"
      },
      "package": "com.hssspares.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite",
      "expo-secure-store",
      "expo-maps",
      "expo-local-authentication",
      "expo-calendar",
      "expo-location",
      [
        "expo-notifications",
        {
          "iosDisplayInForeground": true,
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f1a4aec7-e3ba-437c-a7ca-f19e7ce72751"
      },
      "nextDayDeliveryCharge": 8.50,
      "vatRate": 20
    },
    "owner": "kevmoore1975"
  }
}